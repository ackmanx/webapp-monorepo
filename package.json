{
    "name": "webapp-monorepo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "yarn clean; yarn pinyin:build:prod",
        "clean": "rm -rf public",
        "prettier": "prettier --config ./prettier.config.js --write './**/*.{js,json}'",
        "start": "node bin/www.js",
        "pinyin:build:prod": "yarn pinyin:clean; export APP=pinyin; export NODE_ENV=production; webpack",
        "pinyin:build:watch": "yarn pinyin:clean; export APP=pinyin; webpack --watch",
        "pinyin:clean": "rm -rf public/pinyin"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "compression": "1.7.4",
        "cookie-parser": "1.4.5",
        "copy-webpack-plugin": "6.0.3",
        "debug": "4.1.1",
        "ejs": "3.1.3",
        "express": "4.17.1",
        "http-errors": "1.8.0",
        "lowdb": "1.0.0",
        "morgan": "1.10.0",
        "node-fetch": "2.6.0",
        "querystring": "0.2.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-redux": "7.2.0",
        "react-responsive": "8.1.0",
        "redux": "4.0.5",
        "redux-thunk": "2.3.0",
        "url-loader": "4.1.0",
        "uuid": "8.2.0"
    },
    "devDependencies": {
        "@babel/cli": "7.10.4",
        "@babel/core": "7.10.4",
        "@babel/polyfill": "7.10.4",
        "@babel/preset-env": "7.10.4",
        "@babel/preset-react": "7.10.4",
        "babel-loader": "8.1.0",
        "css-loader": "3.6.0",
        "prettier": "2.0.5",
        "redux-devtools-extension": "2.13.8",
        "style-loader": "1.2.1",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.12"
    }
}
