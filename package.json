    "private": true,
    "scripts": {
        "dict:build:prod": "npm run dict:clean; export APP=dict; export NODE_ENV=production; webpack",
        "dict:build:watch": "npm run dict:clean; export APP=dict; webpack --watch",
        "dict:clean": "rm -rf public/dict",
