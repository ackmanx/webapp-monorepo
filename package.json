{
    "name": "webapp-monorepo",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "pinyin:build:prod": "yarn pinyin:clean; export APP=pinyin; export NODE_ENV=production; webpack",
        "pinyin:build:watch": "yarn pinyin:clean; export APP=pinyin; webpack --watch",
        "pinyin:clean": "rm -rf public/pinyin",
        "vocab:build:prod": "yarn vocab:clean; export APP=vocab; export NODE_ENV=production; webpack",
        "vocab:build:watch": "yarn vocab:clean; export APP=vocab; webpack --watch",
        "vocab:clean": "rm -rf public/vocab",
        "start": "node bin/www.js"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "compression": "1.7.4",
        "cookie-parser": "1.4.5",
        "copy-webpack-plugin": "5.0.3",
        "debug": "4.1.1",
        "ejs": "3.1.3",
        "express": "4.17.1",
        "http-errors": "1.8.0",
        "lowdb": "1.0.0",
        "morgan": "1.10.0",
        "node-fetch": "2.6.0",
        "querystring": "0.2.0",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-redux": "7.1.0",
        "react-responsive": "7.0.0",
        "redux": "4.0.4",
        "redux-thunk": "2.3.0",
        "url-loader": "2.1.0",
        "uuid": "8.2.0"
    },
    "devDependencies": {
        "@babel/cli": "7.5.5",
        "@babel/core": "7.5.5",
        "@babel/polyfill": "7.4.4",
        "@babel/preset-env": "7.5.5",
        "@babel/preset-react": "7.0.0",
        "babel-loader": "8.0.6",
        "css-loader": "3.1.0",
        "redux-devtools-extension": "2.13.8",
        "style-loader": "0.23.1",
        "webpack": "4.36.1",
        "webpack-cli": "3.3.6"
    }
}
